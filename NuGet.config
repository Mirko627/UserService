<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<packageSources>
		<add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
		<add key="Mirko627" value="https://nuget.pkg.github.com/Mirko627/index.json" protocolVersion="3" />
	</packageSources>
	<packageSourceMapping>
		<packageSource key="nuget.org">
			<package pattern="*" />
		</packageSource>
		<packageSource key="Mirko">
			<package pattern="*" />
		</packageSource>
	</packageSourceMapping>
	<packageSourceCredentials>
		<Mirko627>
			<add key="Username" value="%MIRKO_NUGET_USERNAME%" />
			<add key="ClearTextPassword" value="%MIRKO_NUGET_PWD%" />
		</Mirko627>
	</packageSourceCredentials>
</configuration>
